{
	"env": {
		"commonjs": true,
		"es6": true,
		"node": true
	},
	"extends": "airbnb-base",
	"globals": {
		"Atomics": "readonly",
		"SharedArrayBuffer": "readonly"
	},
	"parserOptions": {
		"ecmaVersion": 2018
	},
	"rules": {
		"indent": [1, "tab"],
		"no-tabs": 0,
		"arrow-parens": [2, "as-needed"]
	}
}
